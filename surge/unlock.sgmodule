获取京东Cookie = type=http-request,requires-body=1,pattern=^https:\/\/(api\.m|me-api|ms\.jr)\.jd\.com\/(client\.action\?functionId=signBean|user_new\/info\/GetJDUserInfoUnion\?|gw\/generic\/hy\/h5\/m\/appSign\?),script-path=https://raw.githubusercontent.com/NobyDa/Script/master/JD-DailyBonus/JD_DailyBonus.js



[Panel]
# 附带重载配置,Surge Pro标题,可显示启动时间,点击刷新为重载配置
SurgePro_ReloadProfile = script-name=SurgePro_ReloadProfile,update-interval=1
# 流量统计 点击以切换网络界面
TrafficStatistics = script-name=TrafficStatistics,update-interval=1
# 網路詳情 标题显示为根节点名
# 应当修改的字段 group 代理策略组名称
NET_info = script-name=NET_info,update-interval=1
# 机场信息
# 必须修改的字段:你encode后的机场订阅链接
Sub_info 1 = script-name=Sub_info1,update-interval=1800
Sub_info 2 = script-name=Sub_info2,update-interval=1800

[mitm]
hostname = bmall.camera360.com, vsco.co, api.revenuecat.com, ap*.intsig.net, m*.bybutter.com, api.picsart.c*, api.meiease.c*, api.polaxiong.com, api.vuevideo.net, account.wps.cn, ms.jr.jd.com, me-api.jd.com, api.m.jd.com
